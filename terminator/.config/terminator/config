[global_config]
[keybindings]
  go_up = <Alt>w
  go_down = <Alt>s
  go_left = <Alt>a
  go_right = <Alt>d
  next_tab = <Alt>e
  prev_tab = <Alt>q
  hide_window = <Super>t
[profiles]
  [[default]]
    scrollback_lines = 50000
[layouts]
  [[default]]
    [[[child1]]]
      parent = window0
      type = Terminal
    [[[window0]]]
      parent = ""
      type = Window
[plugins]
